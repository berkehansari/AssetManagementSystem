<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" styleClass="root" stylesheets="@../../../../background.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prototest2.AssetInfo">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <Label fx:id="nameLabel" text="Name: " textFill="WHITE" />
    <Label fx:id="quantityLabel" text="Quantity: " textFill="WHITE" />
    <Label fx:id="typeLabel" text="Type: " textFill="WHITE" />
    <Label fx:id="reportedLabel" text="Reported: " textFill="WHITE" />
    <Label fx:id="usernameLabel" text="Account Username: " textFill="WHITE" />
    <Label fx:id="locationLabel" text="Location: " textFill="WHITE" />
    <Label fx:id="expenseLabel" text="Expense: " textFill="WHITE" />
    <Label fx:id="currentValueLabel" text="Current Value: " textFill="WHITE" />
    <Label fx:id="purchaseValueLabel" text="Purchase Value: " textFill="WHITE" />
    <Label fx:id="interestLabel" text="Interest: " textFill="WHITE" />
    <LineChart fx:id="historyChart" style="-fx-background-color: #ffffff;">
        <xAxis>
            <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
            <NumberAxis side="LEFT" />
        </yAxis>
    </LineChart>


</VBox>
