<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="277.0" prefWidth="223.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prototest2.UpdateAssetForm">
    <children>
      <Label text="Asset Name" />
        <TextField fx:id="assetNameField" prefHeight="25.0" prefWidth="173.0" promptText="Asset Name" />
      <Label text="Quantity" />
        <TextField fx:id="quantityField" promptText="Quantity" />
      <Label text="Type" />
        <TextField fx:id="typeField" promptText="Type" />
      <Label text="Reported" />
        <TextField fx:id="reportedField" promptText="Reported" />
      <Label text="Who is added" />
        <TextField fx:id="userNameField" promptText="Account User Name" />
      <Label text="Where is at?" />
        <TextField fx:id="whereField" promptText="Where" />
        <Button onAction="#handleSaveButtonAction" text="Save" />
    </children>
</VBox>
